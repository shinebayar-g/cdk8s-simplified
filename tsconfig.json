{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "sourceMap": false,
    "declaration": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "noEmit": true,
    "importHelpers": true,
    "lib": ["ES2022"],
    "target": "ES2022",
    "module": "Es2022",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {}
  },
  "exclude": ["node_modules", "tmp"]
}
